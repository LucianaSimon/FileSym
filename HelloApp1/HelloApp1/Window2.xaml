<Window x:Class="HelloApp1.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        xmlns:local="clr-namespace:HelloApp1"
        mc:Ignorable="d"
        Title="Resultados" Height="310" Width="600">
    <Canvas>
        <DockPanel Width="600">
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="_Archivo">
                    <MenuItem Header="_Exportar" Click="MenuItem_Click" />
                    <MenuItem Header="_Salir" Click="MenuItem_Click_1" />
                </MenuItem>
            </Menu>
        </DockPanel>
        <Label HorizontalAlignment="Left" Content="Indicadores del sistema:" Style="{StaticResource lblDesign}" FontSize="22" FontFamily="/HelloApp1;component/Font/#Ubuntu" Foreground="#FFFF422C" Width="294" FontWeight="Bold" Canvas.Left="25" Canvas.Top="26"/>
        <TextBlock x:Name="info_resultados" Height="214" TextWrapping="Wrap" FontSize="13" FontFamily="/HelloApp1;component/Font/#Ubuntu" Foreground="#FF2C2C2C" Width="557" FontWeight="Bold" Canvas.Left="25" Canvas.Top="66"/>

        <local:UserControl1 x:Name="grafico" Width="150" Height="150" Canvas.Left="403" Canvas.Top="49" />

    </Canvas>
</Window>
